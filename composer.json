{
    "name": "meeshalk/ebay-scraper",
    "description": "A simple ebay price and quantity scraper class",
    "keywords": [
        "ebay",
        "scraper",
        "price",
        "quantity",
        "availability"
    ],
    "homepage": "https://github.com/meeshalk/ebay-scraper",
    "license": "MIT",
    "authors": [
        {
            "name": "Meeshal k",
            "email": "meeshal1994@gmail.com",
            "homepage": "http://techkumar.in",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^7.2",
        "soundasleep/html2text": "~1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Meeshal\\Scraper\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Meeshal\\Scraper\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
